{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
, "app/about/page.js", "app/analysis/page.js", "app/calander/page.js", "app/compiler/page.js", "app/components/CFOverviewStats.js", "app/components/Footer.js", "app/components/Nav.js", "app/components/ProblemRatingsChart.js", "app/components/RatingGraph.js", "app/components/TagsAndUnsolved.js", "app/components/YearlyCalendar.js", "app/yourcode/page.js"  ],
  "exclude": ["node_modules"]
}
